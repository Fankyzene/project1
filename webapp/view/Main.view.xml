<mvc:View controllerName="project1.controller.Main"
    xmlns:mvc="sap.ui.core.mvc" displayBlock="true"
    xmlns="sap.m">
    <Page id="page" title="{i18n>title}">
        <VBox id="_IDGenVBox1">
            <Image id="_IDGenImage1" src="https://cdn.shopify.com/s/files/1/0287/4484/3343/files/repair2.png" width="100%" />
        </VBox>
        <content>
            <List id="_IDGenList1" 
            headerText="{i18n>servicesListTitle}"
            class="sapUiResponsiveMargin"
            width="auto"
            items="{services>/Services}" >
            <items>
                <ObjectListItem id="_IDGenObjectListItem1" 
                    title="{services>Name}"
                    type="Navigation"
					press="onPress" />
            </items>
        </List>

        </content>
    </Page>
</mvc:View>
